<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Surname</th>
    <th>City</th>
    <th>Adres</th>
    <th>Phone</th>
    <th>Mail</th>
    <th>Regdate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.surname %></td>
    <td><%= user.city %></td>
    <td><%= user.adres %></td>
    <td><%= user.phone %></td>
    <td><%= user.mail %></td>
    <td><%= user.regdate %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
